<UserControl x:Class="volleyball_stat.ApplicationWindows.VoleyballInformation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <TextBlock Grid.Row="0" Grid.Column="0" Margin="4 10 4 20" Text="Select league: "/>

                <ComboBox Grid.Row="0" Grid.Column="1" Name="leagueCombobox" Margin="4 10 4 20" Height="25"  Width="150" VerticalAlignment="Top" HorizontalAlignment="Left">
                    <ComboBox.Items>
                        <ComboBoxItem>Male</ComboBoxItem>
                        <ComboBoxItem>Female</ComboBoxItem>
                    </ComboBox.Items>
                </ComboBox>
            </Grid>

            <Grid Grid.Row="1">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>

                <TextBlock Grid.Row="0" Grid.Column="0" Margin="4" Text="(Main)Judge 1: "/>
                <TextBox Grid.Row="0" Grid.Column="1" Name="mainJudge" Width="300" Margin="4" HorizontalAlignment="Left"/>

                <TextBlock Grid.Row="1" Grid.Column="0" Margin="4 4 4 20" Text="Judge 2: "/>
                <TextBox Grid.Row="1" Grid.Column="1" Name="secondJudge" Width="300" Margin="4 4 4 20" HorizontalAlignment="Left"/>
            </Grid>

            <Border HorizontalAlignment="Center" VerticalAlignment="Top" BorderBrush="Gray" BorderThickness="1" Grid.Row="2">
                <!--<Grid Grid.Row="2" Grid.RowSpan="2">-->
                <Grid HorizontalAlignment="Center">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Row="0" Grid.Column="0" Margin="4 30 10 30" Text="Team1 name: "/>
                        <TextBox Grid.Row="0" Grid.Column="1" Name="team1Name" Width="150" Margin="3 30 10 30" HorizontalAlignment="Left"/>

                        <TextBlock Grid.Row="0" Grid.Column="2" Margin="4 30 4 30"  Text="Team2 name: "/>
                        <TextBox Grid.Row="0" Grid.Column="3" Name="team2Name" Width="150" Margin="3 30 4 30" HorizontalAlignment="Left"/>
                    </Grid>

                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Grid.Column="0" Margin="4" Text="Player's name"/>
                        <TextBlock Grid.Row="0" Grid.Column="1" Margin="4 4 10 4" Text="#"/>
                        <TextBlock Grid.Row="0" Grid.Column="2" Margin="4" Text="Player's name"/>
                        <TextBlock Grid.Row="0" Grid.Column="3" Margin="4" Text="#"/>

                        <TextBox Grid.Row="1" Grid.Column="0" Name="player1Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Name="player1Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="1" Grid.Column="2" Name="player1Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="1" Grid.Column="3" Name="player1Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="2" Grid.Column="1" Name="player2Name_team1" Width="20" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="2" Grid.Column="2" Name="player2Numb_team1" Width="200" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="2" Grid.Column="3" Name="player2Name_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="2" Grid.Column="0" Name="player2Numb_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="3" Grid.Column="0" Name="player3Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Name="player3Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="3" Grid.Column="2" Name="player3Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="3" Grid.Column="3" Name="player3Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="4" Grid.Column="0" Name="player4Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="4" Grid.Column="1" Name="player4Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="4" Grid.Column="2" Name="player4Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="4" Grid.Column="3" Name="player4Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="5" Grid.Column="0" Name="player5Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="5" Grid.Column="1" Name="player5Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="5" Grid.Column="2" Name="player5Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="5" Grid.Column="3" Name="player5Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="6" Grid.Column="0" Name="player6Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="6" Grid.Column="1" Name="player6Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="6" Grid.Column="2" Name="player6Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="6" Grid.Column="3" Name="player6Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="7" Grid.Column="0" Name="player7Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="7" Grid.Column="1" Name="player7Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="7" Grid.Column="2" Name="player7Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="7" Grid.Column="3" Name="player7Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="8" Grid.Column="0" Name="player8Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="8" Grid.Column="1" Name="player8Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="8" Grid.Column="2" Name="player8Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="8" Grid.Column="3" Name="player8Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="9" Grid.Column="0" Name="player9Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="9" Grid.Column="1" Name="player9Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="9" Grid.Column="2" Name="player9Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="9" Grid.Column="3" Name="player9Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="10" Grid.Column="0" Name="player10Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="10" Grid.Column="1" Name="player10Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="10" Grid.Column="2" Name="player10Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="10" Grid.Column="3" Name="player10Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="11" Grid.Column="0" Name="player11Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="11" Grid.Column="1" Name="player11Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="11" Grid.Column="2" Name="player11Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="11" Grid.Column="3" Name="player11Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>

                        <TextBox Grid.Row="12" Grid.Column="0" Name="player12Name_team1" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="12" Grid.Column="1" Name="player12Numb_team1" Width="20" Margin="4 4 10 4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="12" Grid.Column="2" Name="player12Name_team2" Width="200" Margin="4" HorizontalAlignment="Left"/>
                        <TextBox Grid.Row="12" Grid.Column="3" Name="player12Numb_team2" Width="20" Margin="4" HorizontalAlignment="Left"/>
                    </Grid>
                </Grid>
            </Border>

            <!--<WrapPanel Grid.Row="4">
                <Button Content="Save" Name="Save_AddWindow" Padding="4 8 4 8" Margin="4 10 4 4" Click="Save_AddWindow_Click"></Button>
                <Button Content="Cancel" Name="Cancel_AddWindow" Padding="4 8 4 8" Margin="4 10 4 4"  Click="Cancel_AddWindow_Click"></Button>
            </WrapPanel>-->
        </Grid>
</UserControl>
