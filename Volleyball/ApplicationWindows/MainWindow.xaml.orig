<Window x:Class="volleyball_stat.ApplicationWindows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="VolleyballNet" Height="504.339" Width="776.471" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Grid.Background>
                <ImageBrush ImageSource="C:\Users\Anna\Desktop\TortoisHG\Volleyball Repository\volleyball_stat5_New\volleyball_stat5\volleyball_stat\ApplicationWindows\MainFormBG.jpg"/>
            </Grid.Background>
            <Rectangle HorizontalAlignment="Left" Height="30" Stroke="Black" VerticalAlignment="Top" Width="583" Fill="#FF55A6B8"/>
            <Button Content="Register" Name="RegisterButton" HorizontalAlignment="Left" Margin="12,4,0,0" VerticalAlignment="Top" Width="75" Click="RegisterButton_Click"/>
            <Button Content="Login" Name="LoginButton" HorizontalAlignment="Left" Margin="95,4,0,0" VerticalAlignment="Top" Width="75" Click="LoginButton_Click"/>
        </Grid>
        <Grid Grid.Column="1">

            <StackPanel Background="#FF55A6B8" >
                <Button Content="Add Player" x:Name="AddPlayer" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="AddPlayer_Click"/>
                <Button Content="Add Team" x:Name="AddTeam" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="AddTeam_Click"/>
                <Button Content="Add Game" x:Name="AddGame" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="AddGame_Click"/>
                <Button Content="Calendar" x:Name="Calendar" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="Calendar_Click"/>
                <Button Content="Statistics" x:Name="Statistics" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="Statistics_Click"/>
                <Button Content="Teams" x:Name="Teams" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="Teams_Click"/>
                <Button Content="Referees" x:Name="Referees" Margin="4" Padding="4 20 4 20" Height="60" Width="200" Click="Referees_Click"/>
                <!--<Button Content="Cancel" Name="CancelButton" Margin="4" Padding="4 20 4 20" Width="200" Click="CancelButton_Click"></Button>-->
            </StackPanel>
        </Grid>
    </Grid>
</Window>
